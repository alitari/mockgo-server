{{ $key := .RequestPathParams.key -}}
{{ $body := .RequestBody -}}
{{ $path := .RequestBodyJsonData.path -}}
{{ $val := .RequestBodyJsonData.value -}}
{{ kvStoreAdd $key $path $val -}}

{
    "message": "add kvstore successfully",
    "key": {{ $key | quote }}
    "body": {{ $body | quote }}
    "path": {{ $path | quote }}
    "value": {{ $val | quote }}
}